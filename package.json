{
  "name": "jerryberton",
  "version": "1.0.0",
  "description": "based on hapi framework blog system",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "eslint": "node_modules/.bin/eslint .",
    "start": "cross-env NODE_ENV=development node_modules/.bin/nodemon app.js",
    "prod": "cross-env NODE_ENV=production node_modules/.bin/nodemon app.js",
    "createdb:dev": "node_modules/.bin/sequelize db:create",
    "createdb:prod": "node_modules/.bin/sequelize db:create --env production",
    "createtable": "node_modules/.bin/sequelize db:migrate",
    "createtableundo": "node_modules/.bin/sequelize db:migrate:undo:all",
    "initdata": "node_modules/.bin/sequelize db:seed:all",
    "initdataundo": "node_modules/.bin/sequelize db:seed:undo:all"
  },
  "pre-commit": [
    "eslint"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jerryberton/hapiblog.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/oliyg/jerryberton/issues"
  },
  "homepage": "https://github.com/oliyg/jerryberton#readme",
  "dependencies": {
    "boom": "^7.2.0",
    "cross-env": "^5.2.0",
    "dayjs": "^1.7.5",
    "env2": "^2.2.2",
    "good": "^7.3.0",
    "good-console": "^7.1.0",
    "good-squeeze": "^5.1.0",
    "hapi": "^16.6.3",
    "hapi-auth-jwt2": "^7.4.1",
    "hapi-pagination": "^1.22.0",
    "hapi-redis": "^6.0.0",
    "hapi-swagger": "^7.10.0",
    "inert": "^4.2.1",
    "joi": "^13.6.0",
    "jsonwebtoken": "^8.3.0",
    "mysql2": "^1.6.1",
    "package": "^1.0.1",
    "sequelize": "^4.38.1",
    "vision": "^4.1.1"
  },
  "devDependencies": {
    "eslint": "^5.6.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-react": "^7.11.1",
    "nodemon": "^1.18.4",
    "pre-commit": "^1.2.2",
    "sequelize-cli": "^4.1.1"
  }
}
